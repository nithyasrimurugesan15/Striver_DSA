class Node
{
    int data;
    Node next;
    Node prev;
    Node(int data1,Node next1,Node prev1)
    {
        this.data=data1;
        this.next=next1;
        this.prev=prev1;
    }
     Node(int data1)
    {
        this.data=data1;
        this.next=null;
        this.prev=null;
    }
}
class ll {
    static Node cnvrtdll(int[] arr) 
    {
        Node head=new Node(arr[0]);
        Node prev=head;
        for(int i=1;i<arr.length;i++)
        {
            Node temp=new Node(arr[i],null,prev);
            prev.next=temp;
            prev=temp;
        }
        return head;
    }
     static Node insertbeforekthdll(Node head,int k,int val) 
    {
       if(k==1)
       {
           return insertbeforeheaddll(head,val);
       }
       Node temp=head;
       int c=0;
       while(temp!=null)
       {
           c++;
           if(c==k)
           {
               break;
           }
           temp=temp.next;
       }
       Node nnode=new Node(val);
       Node prevnode=temp.prev;
       prevnode.next=nnode;
       nnode.prev=prevnode;
       nnode.next=temp;
       temp.prev=nnode;
       return head;
       
    }
     static Node insertbeforeheaddll(Node head,int val) 
    {
        Node nnode=new Node(val);
        if(head==null)
        {
            return nnode;
        }
        nnode.next=head;
        head.prev=nnode;
        head=nnode;
        return head;
    }
    static void display(Node head) {
        Node temp=head;
        while(temp!=null)
        {
            System.out.print(temp.data+" -> ");
            temp=temp.next;
        }
         System.out.println("null");
    }
}
public class Main {
    public static void main(String[] args)
    {
        int[] arr={2,3,4,5,6,7};
        Node head=ll.cnvrtdll(arr);
        System.out.println("Before inserting kth in 2d linked list ");
       ll.display(head);
       head=ll.insertbeforekthdll(head,5,10);
       System.out.println("After inserting kth in 2d linked list ");
       ll.display(head);
     
    }
}
