https://leetcode.com/problems/rotate-list/description/

o(n) and o(1)

class Solution {
    public ListNode rotateRight(ListNode head, int k) {
        if(head==null || head.next==null)
        {
            return head;
        }
        ListNode temp=head;
        int l=1;
        while(temp.next!=null)
        {
            temp=temp.next;
            l++;
        }
        if(k%l==0)
        {
            return head;
        }
        k=k%l;
        temp.next=head;
        ListNode lastnewNode=findlast(head,l-k);
        head=lastnewNode.next;
        lastnewNode.next=null;
     return head;
    }
    public ListNode findlast(ListNode head,int diff)
    {
        ListNode temp=head;
        int cnt=1;
        while(temp!=null && diff!=cnt)
        {
            temp=temp.next;
            cnt++;
        }
        return temp;
    }
}


approach 2 : tle

class Solution {
    public ListNode rotateRight(ListNode head, int k) {
        int cnt=0;
        ListNode temp=head;
        while(cnt!=k)
        {
            if(temp==null || temp.next==null)
            {
                return head;
            }
            while(temp.next.next!=null)
            {
                temp=temp.next;
            }
            ListNode nn=temp.next;
            temp.next=null;
            nn.next=head;
            head=nn;
            temp=head;
            cnt++;
        }
        return head;
    }
}
