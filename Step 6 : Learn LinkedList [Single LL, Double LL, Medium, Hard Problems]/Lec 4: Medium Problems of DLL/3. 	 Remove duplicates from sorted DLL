GFG : https://www.geeksforgeeks.org/problems/remove-duplicates-from-a-sorted-doubly-linked-list/1

class Solution {
    Node removeDuplicates(Node head) {
        if(head==null)
        {
            return head;
        }
        Node temp=head;
        while(temp!=null && temp.next!=null)
        {
            Node nn=temp.next;
            while(nn!=null && temp.data==nn.data)
            {
                nn=nn.next;
            }
            temp.next=nn;
            if(nn!=null)
            {
            nn.prev=temp;
            }
            temp=temp.next;
        }
        return head;
    }
}
