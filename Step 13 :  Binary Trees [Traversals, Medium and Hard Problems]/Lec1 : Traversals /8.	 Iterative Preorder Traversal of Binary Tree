Iterative preorder traversal runs in O(n) time and uses O(n) auxiliary space due to the stack

import java.util.*;

// Definition of TreeNode
class TreeNode {
    int val;
    TreeNode left;
    TreeNode right;

    TreeNode(int val) {
        this.val = val;
        left = right = null;
    }
}

class Solution {

    // Iterative Preorder Traversal
    public List<Integer> preorderTraversal(TreeNode root) {
        List<Integer> res = new ArrayList<>();

        if (root == null) {
            return res;
        }

        Stack<TreeNode> st = new Stack<>();
        st.push(root);

        while (!st.isEmpty()) {
            TreeNode node = st.pop();      // Visit root
            res.add(node.val);

            // Push right first so left is processed first
            if (node.right != null) {
                st.push(node.right);
            }
            if (node.left != null) {
                st.push(node.left);
            }
        }
        return res;
    }
}

// Driver Class
public class Main {
    public static void main(String[] args) {

        // Tree creation
        /*
                1
               / \
              2   3
                 /
                5
        */
        TreeNode root = new TreeNode(1);
        root.left = new TreeNode(2);
        root.right = new TreeNode(3);
        root.right.left = new TreeNode(5);

        Solution sol = new Solution();

        // Preorder traversal
        List<Integer> result = sol.preorderTraversal(root);

        // Printing output
        System.out.println("Preorder Traversal (Iterative): " + result);
    }
}
