https://leetcode.com/problems/reverse-words-in-a-string/description/

time : o(n)  and space : o(n)

class Solution {
    public String reverseWords(String s) {
        Stack<String> n=new Stack<>();
        s=s.trim()+" ";
        String st="";
        for(int i=0;i<s.length();i++)
        {
            if(s.charAt(i)==' ')
            {
                if(!st.isEmpty())
                {
                  n.push(st);
                  st="";
                }
            }
            else
            {
                st+=s.charAt(i);
            }
        }
        String ans="";
        while(n.size()!=1)
        {
            ans=ans+n.pop()+" ";
        }
        ans+=n.pop();
        return ans;
    }
}

