
brute : o(n) and o(1)

public class tUf {
  public static int solve(int n, int key, int[] v) {
    int res = -1;
    for (int i = n - 1; i >= 0; i--) {
      if (v[i] == key) {
        res = i;
        break;
      }
    }
    return res;
  }

optimal : o(log n) and o(1)

public class tUf {
  public static int solve(int n, int key, int[] v) {
    int start = 0;
    int end = n - 1;
    int res = -1;

    while (start <= end) {
      int mid = start + (end - start) / 2;
      if (v[mid] == key) {
        res = mid;
        start = mid + 1;

      } else if (key < v[mid]) {
        end = mid - 1;
      } else {
        start = mid + 1;
      }
    }
    return res;
  }

another : use ub -1

o(logn) and o(1)

public int lastOccurrence(int[] nums, int t) {
    int s = 0;
    int e = nums.length - 1;

    while (s <= e) {
        int m = (s + e) / 2;

        if (nums[m] > t) {
            e = m - 1;
        } else {
            s = m + 1;
        }
    }

    int last = s - 1; 

    if (last >= 0 && nums[last] == t)
        return last;
    else
        return -1; // not found
}

