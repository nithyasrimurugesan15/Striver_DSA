https://leetcode.com/problems/binary-search/

time : o(log n) and space : o(1)

class Solution {
    public int search(int[] nums, int target) 
    {
     int s=0;
     int e=nums.length-1;
     while(s<=e)
     {
        int m=(s+e)/2;
        if(nums[m]==target)
        {
            return m;
        }
        else if(target>nums[m])
        {
            s=m+1;
        }
        else
        {
            e=m-1;
        }
     }  
     return -1;  
    }
}
