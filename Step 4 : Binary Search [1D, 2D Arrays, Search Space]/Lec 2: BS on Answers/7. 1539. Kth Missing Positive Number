https://leetcode.com/problems/kth-missing-positive-number/

Brute : o(n) and o(1)

class Solution {
    public int findKthPositive(int[] arr, int k) {
        for(int i=0;i<arr.length;i++)
        {
            if(arr[i]<=k)
            {
                k++;
            }
            else
            {
                break;
            }
        }
        return k;
    }
}

optimal : o(log n) and o(1)

class Solution {
    public int findKthPositive(int[] arr, int k) 
    {
        int l=0;
        int r=arr.length-1;
        while(l<=r)
        {
            int m=(l+r)/2;
            if(arr[m]-m-1<k)
            {
                l=m+1;
            }
            else
            {
                r=m-1;
            }
        }
     return l+k;
    }
}

