time : O(max_N x log(log(max_N))) + O(N x log(num))
space :   O(max_N) + O(N x log(num))

import java.util.*;

class Solution {
    private static final int MAX_N = 100000;
    private int[] SPF = new int[MAX_N + 1];

    public Solution() {
        Arrays.fill(SPF, 1);
    }

    private void sieve() {
        for (int i = 2; i * i <= MAX_N; i++) {
            if (SPF[i] == 1) {
                for (int j = i * i; j <= MAX_N; j += i) {
                    if (SPF[j] == 1) {
                        SPF[j] = i;
                    }
                }
            }
        }
        for (int i = 2; i <= MAX_N; i++) {
            if (SPF[i] == 1) SPF[i] = i; // For remaining primes
        }
    }

    private List<Integer> primeFact(int n) {
        List<Integer> ans = new ArrayList<>();
        while (n != 1) {
            ans.add(SPF[n]);
            n /= SPF[n];
        }
        return ans;
    }

    public List<List<Integer>> primeFactors(int[] queries) {
        sieve();
        List<List<Integer>> ans = new ArrayList<>();
        for (int query : queries) {
            ans.add(primeFact(query));
        }
        return ans;
    }

    public static void main(String[] args) {
        int[] queries = {2, 3, 4, 5, 6};
        Solution sol = new Solution();
        List<List<Integer>> ans = sol.primeFactors(queries);

        for (int i = 0; i < ans.size(); i++) {
            System.out.print("For " + queries[i] + ": ");
            for (int factor : ans.get(i)) {
                System.out.print(factor + " ");
            }
            System.out.println();
        }
    }
}
