https://leetcode.com/problems/subsets/description/

O(N x 2N) and O(N x 2N)

class Solution {
    public List<List<Integer>> subsets(int[] nums) {
        List<List<Integer>> res=new ArrayList<>();
        int l=nums.length;
        int p=(1<<l);
        for(int i=0;i<p;i++)
        {
            List<Integer> sub=new ArrayList<>();
            for(int j=0;j<l;j++)
            {
                if((i&(1<<j))!=0)
                {
                    sub.add(nums[j]);
                }
            }
            res.add(sub);
        }
        return res;
    }
}
