// T.C : O(N)
// S.C : 0(1)

class Main {
    public static void main(String[] args) {
        String bin="1101";
        int dec=2;
        int res=0;
        int p=1;
       for(int i=bin.length()-1;i>=0;i--)
       {
           if(bin.charAt(i)=='1')
           {
               res+=p;
           }
           p*=dec;
       }
        System.out.println(res);
    }
}
