https://leetcode.com/problems/valid-parentheses/description/

o(n) and o(n)

class Solution {
    public boolean isValid(String s) {
        Stack<Character> st = new Stack();
        for(char i : s.toCharArray())
        {
            if(i=='(' || i=='[' || i=='{')
            {
                st.push(i);
            }
            else
            {
                if(st.isEmpty())
                {
                    return false;
                }
                char p=st.pop();
                if(p=='[' && i==']' || p=='(' && i==')' || p=='{' && i=='}')
                {
                    continue;
                }
                else
                {
                    return false;
                }
            }
        }
        return st.isEmpty();
    }
}
