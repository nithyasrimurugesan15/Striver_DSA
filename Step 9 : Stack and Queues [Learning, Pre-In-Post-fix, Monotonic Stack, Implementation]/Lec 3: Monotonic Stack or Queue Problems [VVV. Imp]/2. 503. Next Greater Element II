https://leetcode.com/problems/next-greater-element-ii/description/

1.

o(n2) and o(n)

class Solution 
{
    public int[] nextGreaterElements(int[] nums) 
    {
        int l=nums.length;
        int[] res=new int[l];
        for(int i=0;i<l;i++)
         {
            res[i]=-1;
            for(int j=0;j<l;j++)
            {
                if(nums[(i+j)%l]>nums[i])
                {
                    res[i]=nums[(i+j)%l];
                    break;
                }
            }
         }
         return  res;
    }
}

2.

o(n2) and o(2n)

class Solution 
{
    public int[] nextGreaterElements(int[] nums) 
    {
        int l=nums.length;
         int[] res=new int[l];
         int[] da=new int[l*2];
         for(int i=0;i<l;i++)
         {
            da[i+l]=da[i]=nums[i];
         }
         for(int i=0;i<l;i++)
         {
            res[i]=-1;
            for(int j=i+1;j<da.length;j++)
            {
                if(da[j]>nums[i])
                {
                    res[i]=da[j];
                    break;
                }
            }
         }
         return  res;
    }
}

3.

o(4n) and o(n)

import java.util.*;
class Solution {
    public int[] nextGreaterElements(int[] arr) {
        int n = arr.length;

        int[] ans = new int[n];

        Stack<Integer> st = new Stack<>();

        for (int i = 2 * n - 1; i >= 0; i--) {
            int ind = i % n;
            int currEle = arr[ind];

            while (!st.isEmpty() && st.peek() <= currEle) {
                st.pop();
            }

            if (i < n) {
                if (st.isEmpty()) {
                    ans[ind] = -1;
                } else {
                    ans[ind] = st.peek();
                }
            }

            st.push(currEle);
        }

        return ans;
    }
}

public class Main {
    public static void main(String[] args) {
        int[] arr = {5, 7, 1, 7, 6, 0};

      
        Solution sol = new Solution();
        
        int[] ans = sol.nextGreaterElements(arr);

        System.out.print("The next greater elements are: ");
        for (int num : ans) {
            System.out.print(num + " ");
        }
    }
}


