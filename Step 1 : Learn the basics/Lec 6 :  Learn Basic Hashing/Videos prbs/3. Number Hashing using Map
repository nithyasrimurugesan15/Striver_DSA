T.C = O(n + q) (average)
S.C = O(k) (where k = number of distinct elements)

import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);

        // Input size of array
        int n = input.nextInt();
        int[] arr = new int[n];

        for (int i = 0; i < n; i++) {
            arr[i] = input.nextInt();
        }

        // HashMap for number frequency
        Map<Integer, Integer> hash = new HashMap<>();

        for (int i = 0; i < n; i++) {
            hash.put(arr[i], hash.getOrDefault(arr[i], 0) + 1);
        }

        // Number of queries
        int q = input.nextInt();
        while (q-- > 0) {
            int num = input.nextInt(); // query number
            System.out.println(hash.getOrDefault(num, 0));
        }
    }
}
