T.C. = O(2^n)
S.C. = O(n)

class Main {
    static int fibbo(int n)
    {
        if(n<=1)
        return n;
        int last=fibbo(n-1);
        int slast=fibbo(n-2);
        return last+slast;
    }
    public static void main(String[] args) {
     int n=6;
     System.out.println(fibbo(n));
    }
}
